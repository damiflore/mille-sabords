{
  "name": "mille-sabords",
  "description": "Web version of a dice game called mille sabords",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damiflore/mille-sabords"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./*": "./*"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.jsx,.html,.mjs",
    "dev": "node ./scripts/dev/dev.mjs",
    "test": "node ./scripts/test/test.mjs",
    "build": "node ./scripts/build/build.mjs",
    "build-watch": "node ./scripts/build/build.mjs --watch",
    "preview": "node ./scripts/build/preview.mjs",
    "file-size": "node ./scripts/file_size/file_size.mjs --log",
    "lighthouse": "node ./scripts/lighthouse/lighthouse.mjs --local",
    "performance": "node ./scripts/performance/performance.mjs --log --local",
    "test-with-coverage": "npm run test -- --coverage",
    "start": "npm run build && npm run preview",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./scripts/dev/install_certificate.mjs"
  },
  "dependencies": {
    "@jsenv/logger": "4.0.1",
    "@jsenv/pwa": "5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-syntax-import-assertions": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.16.5",
    "@jsenv/assert": "2.5.4",
    "@jsenv/core": "27.0.0-alpha.59",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/eslint-import-resolver": "0.1.7",
    "@jsenv/file-size-impact": "12.1.12",
    "@jsenv/filesystem": "3.2.2",
    "@jsenv/https-local": "1.0.8",
    "@jsenv/lighthouse-impact": "2.1.11",
    "@jsenv/performance-impact": "2.2.11",
    "@jsenv/plugin-react": "0.0.8",
    "eslint": "8.15.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.0",
    "prettier": "2.6.2"
  }
}
