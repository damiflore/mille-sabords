{
  "name": "mille-sabords",
  "description": "Web version of a dice game called mille sabords",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damiflore/mille-sabords"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/boot/boot.js",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "node ./script/server/start_dev_server.mjs",
    "eslint": "npx eslint . --ext=.js,.jsx,.mjs,.cjs,.html",
    "importmap": "node ./script/importmap/importmap.mjs",
    "lighthouse": "node ./script/lighthouse/lighthouse_report.mjs --local",
    "file-sizes": "node ./script/file_size/file_size_report.mjs --local",
    "performances": "node ./script/performance/performance_report.mjs --local",
    "dist": "npm run build",
    "build": "node ./script/build/build.mjs",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "start-prod": "node ./script/server/start_prod_server.mjs",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./script/server/install_certificate_authority.mjs"
  },
  "dependencies": {
    "@jsenv/logger": "4.0.1",
    "@jsenv/server": "12.4.1",
    "@jsenv/pwa": "4.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/eslint-parser": "7.16.5",
    "@babel/plugin-transform-react-jsx": "7.16.5",
    "@jsenv/assert": "2.4.1",
    "@jsenv/babel-preset": "1.1.2",
    "@jsenv/codecov-upload": "3.5.0",
    "@jsenv/core": "25.4.1",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/file-size-impact": "12.1.6",
    "@jsenv/filesystem": "2.7.1",
    "@jsenv/https-local": "1.0.7",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/importmap-node-module": "5.1.3",
    "@jsenv/lighthouse-impact": "2.1.6",
    "@jsenv/lighthouse-score-impact": "2.6.0",
    "@jsenv/performance-impact": "2.2.6",
    "@jsenv/prettier-check-project": "5.6.1",
    "eslint": "8.7.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.28.0",
    "prettier": "2.5.1"
  }
}
