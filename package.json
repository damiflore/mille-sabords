{
  "name": "mille-sabords",
  "description": "Web version of a dice game called mille sabords",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damiflore/mille-sabords"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./*": "./*"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "build:local": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs",
    "build:prod": "node ./scripts/build.mjs --prod",
    "build:watch": "npm run build:local -- --watch",
    "build:size": "node ./scripts/build_size.mjs --log",
    "eslint": "npx eslint . --ext=.js,.jsx,.html,.mjs,.cjs",
    "prettier": "prettier --write .",
    "lighthouse": "node ./scripts/lighthouse.mjs --local",
    "performance": "node ./scripts/performance.mjs --log --local",
    "test:coverage": "npm run test -- --coverage",
    "playwright:install": "npx playwright install-deps && npx playwright install",
    "certificate:install": "node ./scripts/install_certificate.mjs"
  },
  "dependencies": {
    "@jsenv/logger": "4.1.1",
    "@jsenv/pwa": "6.0.1",
    "@jsenv/service-worker": "1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-syntax-import-assertions": "7.20.0",
    "@babel/plugin-transform-react-jsx": "7.21.0",
    "@jsenv/assert": "2.8.1",
    "@jsenv/core": "34.1.2",
    "@jsenv/eslint-config": "16.3.9",
    "@jsenv/eslint-import-resolver": "0.4.10",
    "@jsenv/file-size-impact": "14.1.0",
    "@jsenv/filesystem": "4.2.3",
    "@jsenv/https-local": "3.0.6",
    "@jsenv/lighthouse-impact": "4.0.3",
    "@jsenv/performance-impact": "4.1.0",
    "@jsenv/plugin-minification": "1.2.2",
    "@jsenv/plugin-placeholders": "1.1.3",
    "@jsenv/plugin-react": "1.2.15",
    "eslint": "8.38.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "open": "9.1.0",
    "playwright": "1.32.3",
    "prettier": "2.8.7"
  }
}
